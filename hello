	PRESERVE8
	THUMB
	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0x20001000
	DCD START 
	ALIGN
	AREA MY_CODE,CODE,READONLY
	ENTRY
	EXPORT START
START
	MOV R2,#0x2222
	MOV R1,#0xABCD
	MUL R2,R1
STOP B STOP 
	END




	PRESERVE8
	THUMB
	AREA RESET,CODE,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0x20001000
	DCD START
	ALIGN
	AREA MY_CODE,CODE,READONLY
	EXPORT START
START
	LDR R0,=0x56784321
	LDR R1,=0x43215678
	LDR R2,=0x11112222
	LDR R3,=0x22223333
	ADDS R4,R0,R2
	ADC R5,R1,R3
	ADC R4,R4,#0
STOP B STOP
	END





	PRESERVE8
	THUMB
	AREA RESET,CODE,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0x20001000
	DCD START
	ALIGN
	AREA MY_CODE,CODE,READONLY
	EXPORT START
START
	MOV R2,#10
	MOV R1,#0
LOOP ADD R1,R2
	SUBS R2,#1
	BNE LOOP
STOP B STOP
	END




	PRESERVE8
	THUMB
	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0x20001000
	DCD START
	ALIGN
	AREA MY_CODE,CODE,READONLY
	EXPORT START
START
	MOV R0,#5
	MOV R1,#1
FACTORIAL __LOOP
	MUL R1,R1,R0
	SUBS R5,R5,#1
	BNE FACTORIAL __LOOP
STOP B STOP
	END
